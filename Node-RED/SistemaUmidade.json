    {
        "id": "f4c1b07862820b58",
        "type": "function",
        "z": "591c1b311c4991f6",
        "name": "Alertas",
        "func": "var payload = msg.payload; \n\n// Temperatura\nvar temperatura = payload.Temperatura;\n// Potenciômetro\nvar umidade = payload.Potenciometro;\n\n// Pegando o valor do slider de temperatura mínima e máxima\nvar tempMinima = flow.get('tempMinima') || 10;  // Valor padrão de 10 se o slider não estiver configurado\nvar tempMaxima = flow.get('tempMaxima') || 30;  // Valor padrão de 30 se o slider não estiver configurado\n// Pegando o valor do slider de umidade mínima e máxima\nvar umidadeMinima = flow.get('umidadeMinima') || 20;  // Valor padrão de 20 se o slider não estiver configurado\nvar umidadeMaxima = flow.get('umidadeMaxima') || 80;  // Valor padrão de 80 se o slider não estiver configurado\n\n\n// Verificação da Temperatura\nif (temperatura < tempMinima) {\n    msg.payload = \"Low Temperature\";\n    return msg;\n} else if (temperatura > tempMaxima) {\n    msg.payload = \"High Temperature\";\n    return msg;\n}\n\n// Verificação da Umidade\nif (umidade < 20) {\n    msg.payload = \"Low Humidity!\";\n    return msg;\n} else if (umidade > 80) {\n    msg.payload = \"High Humidity\";\n    return msg;\n}\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
