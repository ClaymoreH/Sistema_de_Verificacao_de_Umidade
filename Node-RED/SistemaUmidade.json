    {
        "id": "6ccb40eba8af8605",
        "type": "ui_template",
        "z": "591c1b311c4991f6",
        "group": "3b79fa0cf6a3bd7e",
        "name": "",
        "order": 1,
        "width": 0,
        "height": 0,
        "format": "<head>\n  <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.10/css/weather-icons.min.css\">\n  <style>\n    .container {\n      text-align: center;\n      font-family: 'Arial', sans-serif;\n      background-color: #f4f4f4;\n      border-radius: 15px;\n      padding: 20px;\n      width: 300px;\n      box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n      margin: 10px auto;\n    }\n    .container h3 {\n      font-size: 24px;\n      color: #2196F3;\n      margin-bottom: 20px;\n    }\n    .weather-icon {\n      font-size: 80px;\n      color: #2196F3;\n      margin-bottom: 15px;\n    }\n    .weather-info {\n      font-size: 16px;\n      color: #333;\n    }\n    .weather-info strong {\n      color: #000;\n    }\n    .separator {\n      margin: 15px 0;\n      border-top: 1px solid #ddd;\n    }\n    .value {\n      font-size: 18px;\n      color: #555;\n      margin-top: 5px;\n    }\n  </style>\n</head>\n\n<div class=\"container\">\n  <h3>Clima Atual</h3>\n\n  <!-- Exibe o ícone com base na condição -->\n  <div class=\"weather-icon\">\n    <i class=\"wi\" ng-class=\"{\n      'wi-day-sunny': msg.payload.Condition == 'Sunny',\n      'wi-day-cloudy': msg.payload.Condition == 'Partly cloudy',\n      'wi-cloud': msg.payload.Condition == 'Cloudy',\n      'wi-rain': msg.payload.Condition == 'Rainy',\n      'wi-snow': msg.payload.Condition == 'Snow'\n    }\"></i>\n  </div>\n\n  <!-- Exibe outras informações -->\n  <div class=\"weather-info\">\n    <p><strong>Local:</strong> <span class=\"value\">{{msg.payload.Location}}, {{msg.payload.Region}}, {{msg.payload.Country}}</span></p>\n    <div class=\"separator\"></div>\n    <p><strong>Temperatura:</strong> <span class=\"value\">{{msg.payload.Temperature}} °C</span></p>\n    <div class=\"separator\"></div>\n    <p><strong>Condição:</strong> <span class=\"value\">{{msg.payload.Condition}}</span></p>\n  </div>\n</div>\n",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
